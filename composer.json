{
    "name": "broeser/feeld",
    "description": "Feeld provides typed field objects that can be used as building blocks to create CLI questionnaires, HTML forms, and much more. Includes sanitization and validation.",
    "keywords": ["form", "question", "field", "sanitization", "validation"],
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "broeser/sanitor": "dev-master",
        "broeser/wellid": ">=0.4.0"
    },
    "suggests": {
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "symfony/console": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Feeld\\": "src/",
            "FeeldUsageExamples\\": "examples/"
        }
    }
}
